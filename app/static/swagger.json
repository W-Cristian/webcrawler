              "$ref": "#/components/parameters/key"
            },
            {
              "$ref": "#/components/parameters/ACCESS_TOKEN"
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "content": {
                "application/json": {
                  "schema": {
                      "$ref": "#/components/schemas/freelanceRespose"
                    }
                  }
                }
            },
            "210": {                
              "description": "invalid ACCESS_TOKEN",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "keyword": {
                        "type": "string"
                      },
                      "ACCESS_TOKEN": {
                        "type": "string"
                      },
                      "status": {
                        "type": "string"
                      }
                    }
                  }
